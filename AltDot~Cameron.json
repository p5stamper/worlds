:false,"isMediaFile":false,"hidden":false,"exported":true,"zIndex":779,"isBlob":false,"codeSize":14,"icon":"./static/media/tool.d68b9b61.svg","lineHighLightingStatus":"none"},{"id":"g9ll704t0","name":"draw","code":"\n  \n  // update xPos and background\n  background(175, 112, 248, 10)\n  xPos = (xPos +2) % width\n  \n  // draw a row of dots\n  for(var i = 0; i < height/maxSize; i++){\n    sizeRect(xPos, i*maxSize )\n }\n  \n","args":"","x":1044.5219559177547,"y":2319.8471369669273,"editorWidth":300,"editorHeight":200,"iframeWidth":200,"iframeHeight":200,"isIndex":false,"isTxtFile":false,"isMediaFile":false,"hidden":false,"exported":true,"zIndex":778,"isBlob":false,"codeSize":14,"icon":"./static/media/tool.d68b9b61.svg","lineHighLightingStatus":"none"},{"id":"zqy9v8a2q","name":"sizeRect","code":"\n  // generate a random size\n  var size = random(0, 50)\n  \n  // set the opacity\n  var opacity = map(size, 0, 25, 0, 255)\n  \n  // draw a dot   \n  fill(255, opacity)\n  rect(x,y,size, size)\n","args":"x=mouseX, y=mouseY","x":1628.2714203887335,"y":1874.2586086436847,"editorWidth":300,"editorHeight":200,"iframeWidth":200,"iframeHeight":200,"isIndex":false,"isTxtFile":false,"isMediaFile":false,"hidden":false,"exported":true,"zIndex":780,"isBlob":false,"codeSize":14,"icon":"./static/media/box.310d8273.svg","lineHighLightingStatus":"none"},{"id":"uerysulkf","name":"sizeDot","code":"\n  // generate a random size\n  var size = random(0, maxSize)\n  \n  // set the opacity\n  var opacity = map(size, 0, maxSize, 0, 255)\n  \n  // draw a dot   \n  fill(255, opacity)\n  ellipse(x,y,size, size)\n","args":"x=mouseX, y=mouseY","x":1592.1345487786257,"y":2281.4933540960587,"editorWidth":300,"editorHeight":200,"iframeWidth":200,"iframeHeight":200,"isIndex":false,"isTxtFile":false,"isMediaFile":false,"hidden":true,"exported":true,"zIndex":774,"isBlob":false,"codeSize":14,"icon":"./static/media/box.310d8273.svg","lineHighLightingStatus":"none"}],"scale":0.7194867414236068,"originX":-455.50060533447476,"originY":-1164.7019088924967,"worldKey":"AltDot~Cameron","worldEdited":false,"worldPublishTime":"Wed Feb 19 2020 04:21:46 GMT-0800 (Pacific Standard Time)","snapToGrid":false,"linesOn":true,"js":"\nvar xPos = 0\nvar maxSize = 20\n\n\nfunction setup(x=50, y=50, lastX = 0, lastY = 0){\n  \n    createCanvas(200, 200)\n    background(175, 112, 248)\n    noStroke()\n  \n}\n\nfunction draw(){\n  \n    \n    // update xPos and background\n    background(175, 112, 248, 10)\n    xPos = (xPos +2) % width\n    \n    // draw a row of dots\n    for(var i = 0; i < height/maxSize; i++){\n      sizeRect(xPos, i*maxSize )\n   }\n    \n  \n}\n\nfunction sizeRect(x=mouseX, y=mouseY){\n  \n    // generate a random size\n    var size = random(0, 50)\n    \n    // set the opacity\n    var opacity = map(size, 0, 25, 0, 255)\n    \n    // draw a dot   \n    fill(255, opacity)\n    rect(x,y,size, size)\n  \n}\n\nfunction sizeDot(x=mouseX, y=mouseY){\n  \n    // generate a random size\n    var size = random(0, maxSize)\n    \n    // set the opacity\n    var opacity = map(size, 0, maxSize, 0, 255)\n    \n    // draw a dot   \n    fill(255, opacity)\n    ellipse(x,y,size, size)\n  \n}\n","highlightedLines":{}}