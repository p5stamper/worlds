idden":false,"exported":true,"zIndex":685,"isBlob":true,"codeSize":14,"icon":"./static/media/globe.50d70b6d.svg","lineHighLightingStatus":"none"},{"id":"ld3s5t8ag","name":"//  Acceleratio...","code":"//  Acceleration\nlet ax = 0;\nlet ay = 0;","args":" ","x":806.8898753613661,"y":1108.6025709341666,"editorWidth":300,"editorHeight":200,"iframeWidth":0,"iframeHeight":0,"isIndex":false,"isTxtFile":false,"isMediaFile":false,"hidden":false,"exported":true,"zIndex":686,"isBlob":true,"codeSize":14,"icon":"./static/media/globe.50d70b6d.svg","lineHighLightingStatus":"none"},{"id":"ih56seu9m","name":"let vMultiplier...","code":"let vMultiplier = 0.007;\nlet bMultiplier = 0.6;","args":" ","x":806.8898753613661,"y":1408.6025709341666,"editorWidth":300,"editorHeight":200,"iframeWidth":0,"iframeHeight":0,"isIndex":false,"isTxtFile":false,"isMediaFile":false,"hidden":false,"exported":true,"zIndex":687,"isBlob":true,"codeSize":14,"icon":"./static/media/globe.50d70b6d.svg","lineHighLightingStatus":"none"},{"id":"bmays4lci","name":"setup","code":"\n  createCanvas(displayWidth, displayHeight);\n  fill(0);\n","args":"","x":806.8898753613661,"y":1708.6025709341666,"editorWidth":300,"editorHeight":200,"iframeWidth":100,"iframeHeight":100,"isIndex":false,"isTxtFile":false,"isMediaFile":false,"hidden":false,"exported":true,"zIndex":688,"isBlob":false,"codeSize":14,"icon":"./static/media/tool.d68b9b61.svg","lineHighLightingStatus":"none"},{"id":"2qwhs2506","name":"draw","code":"\n  background(255);\n  ballMove();\n  ellipse(x, y, 30, 30);\n","args":"","x":806.8898753613661,"y":2088.6025709341666,"editorWidth":300,"editorHeight":200,"iframeWidth":100,"iframeHeight":100,"isIndex":false,"isTxtFile":false,"isMediaFile":false,"hidden":false,"exported":true,"zIndex":689,"isBlob":false,"codeSize":14,"icon":"./static/media/tool.d68b9b61.svg","lineHighLightingStatus":"none"},{"id":"7mijlt0oi","name":"ballMove","code":"\n  ax = accelerationX;\n  ay = accelerationY;\n\n  vx = vx + ay;\n  vy = vy + ax;\n  y = y + vy * vMultiplier;\n  x = x + vx * vMultiplier;\n\n  // Bounce when touch the edge of the canvas\n  if (x < 0) {\n    x = 0;\n    vx = -vx * bMultiplier;\n  }\n  if (y < 0) {\n    y = 0;\n    vy = -vy * bMultiplier;\n  }\n  if (x > width - 20) {\n    x = width - 20;\n    vx = -vx * bMultiplier;\n  }\n  if (y > height - 20) {\n    y = height - 20;\n    vy = -vy * bMultiplier;\n  }\n","args":"","x":806.8898753613661,"y":2468.6025709341666,"editorWidth":300,"editorHeight":200,"iframeWidth":100,"iframeHeight":100,"isIndex":false,"isTxtFile":false,"isMediaFile":false,"hidden":false,"exported":true,"zIndex":690,"isBlob":false,"codeSize":14,"icon":"./static/media/box.310d8273.svg","lineHighLightingStatus":"none"}],"scale":1,"originX":-521.5432232678384,"originY":-319.6558852454648,"worldKey":"Ball_(From_p5_ref)~Cameron","worldEdited":false,"worldPublishTime":"Sat Feb 29 2020 20:35:48 GMT-0800 (PST)","snapToGrid":false,"linesOn":false,"js":"\n//  Position Variasbles\nlet x = 0;\nlet y = 0;\n\n\n\n//  Speed - Velocity\nlet vx = 0;\nlet vy = 0;\n\n\n\n//  Acceleration\nlet ax = 0;\nlet ay = 0;\n\n\n\nlet vMultiplier = 0.007;\nlet bMultiplier = 0.6;\n\n\nfunction setup(){\n  \n    createCanvas(displayWidth, displayHeight);\n    fill(0);\n  \n}\n\nfunction draw(){\n  \n    background(255);\n    ballMove();\n    ellipse(x, y, 30, 30);\n  \n}\n\nfunction ballMove(){\n  \n    ax = accelerationX;\n    ay = accelerationY;\n  \n    vx = vx + ay;\n    vy = vy + ax;\n    y = y + vy * vMultiplier;\n    x = x + vx * vMultiplier;\n  \n    // Bounce when touch the edge of the canvas\n    if (x < 0) {\n      x = 0;\n      vx = -vx * bMultiplier;\n    }\n    if (y < 0) {\n      y = 0;\n      vy = -vy * bMultiplier;\n    }\n    if (x > width - 20) {\n      x = width - 20;\n      vx = -vx * bMultiplier;\n    }\n    if (y > height - 20) {\n      y = height - 20;\n      vy = -vy * bMultiplier;\n    }\n  \n}\n","highlightedLines":{}}