 xPos = 0;\nvar maxSize = 20;\n\n\nfunction sizeDot(x = mouseX, y = mouseY){\n  \n    // generate a random size\n    var size = random(0, maxSize);\n  \n    // set the opacity\n    var opacity = map(size, 0, maxSize, 0, 255);\n  \n    // draw a dot\n    fill(255, opacity);\n    ellipse(x, y, size, size);\n}\n","highlightedLines":{}}